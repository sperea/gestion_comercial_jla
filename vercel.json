{
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "^/api/(.*)$",
      "dest": "/api/$1"
    },
    {
      "src": "^/$",
      "dest": "/login",
      "status": 307
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}